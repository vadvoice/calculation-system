<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        x:Class="c_sharp.Views.ResultsWindow"
        Title="Результати розрахунку"
        Width="400" Height="400">
    <StackPanel Margin="20">
        <TextBlock Text="Результати розрахунку" FontSize="20" Margin="0,0,0,20"/>

        <ListBox Height="250">
            <ListBoxItem>Ітерація 1: Значення = 42.5</ListBoxItem>
            <ListBoxItem>Ітерація 2: Значення = 38.7</ListBoxItem>
            <ListBoxItem>Ітерація 3: Значення = 45.2</ListBoxItem>
            <ListBoxItem>Ітерація 4: Значення = 41.9</ListBoxItem>
            <ListBoxItem>Ітерація 5: Значення = 43.1</ListBoxItem>
        </ListBox>

        <Button Content="Розпочати новий розрахунок" 
                Click="OnNewCalculationClick"
                HorizontalAlignment="Right" 
                Margin="0,20,0,0"/>
    </StackPanel>
</Window> 